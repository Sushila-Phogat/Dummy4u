        "if __name__ == '__main__':\n",
        "    N = int(input())\n",
        "    ip=[]\n",
        "    lt=[]\n",
        "    for i in range(N):\n",
        "        ip.append(input())\n",
        "    for k in ip:\n",
        "        j=k.split(\" \")\n",
        "        if j[0]=='insert':\n",
        "            lt.insert(int(j[1]),int(j[2]))\n",
        "        elif j[0]==\"append\":\n",
        "            lt.append(int(j[1]))\n",
        "        elif j[0]==\"remove\":\n",
        "            lt.remove(int(j[1]))\n",
        "        elif j[0]==\"pop\":\n",
        "            lt.pop()\n",
        "        elif j[0]==\"sort\":\n",
        "            lt.sort()\n",
        "        elif j[0]==\"reverse\":\n",
        "            lt.reverse()\n",
        "        elif j[0]==\"print\":\n",
        "            print(lt)\n",
        "        else:\n",
        "            pass\n",
        "    \n",
        "        \n"
